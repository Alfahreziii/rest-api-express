{
  "name": "rest-api",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon src/index.js  --watch src/ --watch package.json --watch .env",
    "make:migration": "npx sequelize-cli migration:generate --name",
    "make:seed": "npx sequelize-cli seed:generate --name",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:up": "npx sequelize-cli db:migrate:up",
    "migrate:down": "npx sequelize-cli db:migrate:down",
    "seed": "npx sequelize-cli db:seed:all",
    "start": "node src/index.js"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "multer": "^1.4.5-lts.2",
    "mysql2": "^3.14.1",
    "nodemailer": "^7.0.0",
    "sequelize": "^6.37.7",
    "uuid": "^11.1.0",
    "validator": "^13.15.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.10"
  }
}
